local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("Slap Tower")

local KillingCheats = PhantomForcesWindow:NewSection("Credit To TheMugenKing")
local KillingCheats = PhantomForcesWindow:NewSection("YouTube TheMugenKing")
local KillingCheats = PhantomForcesWindow:NewSection("Subscribe")

KillingCheats:CreateButton("Auto Spam Infinity", function()

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer

local SpawnList = {
    Vector3.new(33.57, 169.75, 32.66),
    Vector3.new(33.30, 168.92, 44.43),
    Vector3.new(33.49, 169.29, 57.48),
    Vector3.new(38.81, 170.56, 86.66),
}

local function startTeleportLoop(char)
    local hrp = char:WaitForChild("HumanoidRootPart")
    coroutine.wrap(function()
        while char and char.Parent do
            for _, pos in ipairs(SpawnList) do
                hrp.CFrame = CFrame.new(pos)
                task.wait()
            end
        end
    end)()
end

-- Jalankan saat karakter ada
if player.Character then
    startTeleportLoop(player.Character)
end

-- Ulangi kalau respawn
player.CharacterAdded:Connect(function(char)
    startTeleportLoop(char)
end)

end)

KillingCheats:CreateButton("AutoPlay To End", function()

 -- Auto-Generated Script by @TheMugenKing
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local hrp = player.Character:WaitForChild("HumanoidRootPart")

local SpawnList = {
    Vector3.new(-46.07, -3.70, 22.11),
    Vector3.new(-36.55, -0.70, 19.44),
    Vector3.new(-25.68, 2.30, 19.95),
    Vector3.new(-168.36, 778.32, 60.39),
}

for i, pos in ipairs(SpawnList) do
    task.wait(0.4)
    hrp.CFrame = CFrame.new(pos)
end

end)
